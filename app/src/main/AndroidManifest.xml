<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anthony.flightbookerfinal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register"/>
        <activity android:name=".User"/>
        <activity android:name=".Admin"/>
        <activity android:name=".BookedItineraries"/>
        <activity android:name=".EditProfile"/>
        <activity android:name=".ClientList"/>
        <activity android:name=".ItineraryDetail"/>
        <activity android:name=".PossibleItinerary"/>
        <activity android:name=".BillingInfo"/>
        <activity android:name=".ItineraryList"/>
    </application>

</manifest>